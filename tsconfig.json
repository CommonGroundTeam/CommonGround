{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts",
    "app/index.jsx",
    "app/+not-found.jsx",
    "app/(tabs)/Profile/ProfilePage.jsx",
    "app/(tabs)/teams.jsx",
    "app/(tabs)/settings.jsx",
    "app/(tabs)/index.jsx",
    "app/(tabs)/explore.jsx",
    "app/(tabs)/chat.jsx",
    "app/(tabs)/_layout.jsx",
    "app/(tabs)/calendar.jsx",
    "app/_layout.jsx"
  ]
}
